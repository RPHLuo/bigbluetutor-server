    "deepstream.io": "^3.1.1",
    "deepstream.io-client-js": "^2.2.1",
    "deepstream.io-provider-search-rethinkdb": "^2.0.0",
    "deepstream.io-storage-rethinkdb": "^1.0.2",
    "dotenv": "^4.0.0",
    "google-auth-library": "^0.11.0",
