{
  "name": "bigbluetutor",
  "version": "1.0.0",
  "description": "Backend for BigBlueTutor",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server": "node server.js",
    "start:client": "sleep 5 && node client.js",
    "start:provider-search": "sleep 5 && node providerSearch.js",
    "start:all": "npm run start:server && npm run start:client && npm run start:provider-search",
    "start": "npm run start:all",
    "db:cleartables": "sleep 2 && python3 ./scripts/cleartables.py",
    "db:createtables": "node createtables.js && npm run db:cleartables"
  },
